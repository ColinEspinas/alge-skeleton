{
	"name": "alge-skeleton",
	"version": "1.0.0",
	"description": "A skeleton template for games made with alge.",
	"source": "src/*.ts",
	"main": "dist/index.js",
	"browser": "dist/index.umd.js",
	"scripts": {
		"dev": "webpack --mode=development --watch",
		"build": "webpack --mode=production",
		"nw": "npm run build & nw ./"
	},
	"window": {
		"title": "New Game",
		"width": 720,
		"height": 480
	},
	"chromium-args": "--disable-devtools",
	"keywords": [],
	"author": "Colin Espinas",
	"license": "MIT",
	"devDependencies": {
		"fork-ts-checker-webpack-plugin": "^6.2.1",
		"nw": "^0.46.4",
		"ts-loader": "^8.1.0",
		"typescript": "^3.9.9",
		"webpack": "^5.31.2",
		"webpack-cli": "^4.6.0",
		"webpackbar": "^5.0.0-3"
	},
	"dependencies": {
		"alge": "file:../alge"
	}
}
